---
import DownloadLayout from "../layouts/DownloadLayout.astro"
import { DownloadList, DownloadListItem } from "../components"

const title = "SVG services"
const description = "SVG services â€“ SVG rendering library for foobar2000"
const repo = "svg-services"
---

<DownloadLayout title={title} metaDescription={description} repo={repo}>
  <p>
    SVG services allows other compatible foobar2000 components to render SVG
    files.
  </p>

  <h2>Downloads</h2>
  <h3>Latest version</h3>
  <DownloadList>
    <DownloadListItem
      filename="foo_svg_services-0.4.0.x86-x64.fb2k-component"
      label="Version 0.4.0"
    />
  </DownloadList>

  <h3>Previous version</h3>
  <DownloadList>
    <DownloadListItem
      filename="foo_svg_services-0.3.0.x86-x64.fb2k-component"
      label="Version 0.3.0"
    />
    <DownloadListItem
      filename="foo_svg_services-0.2.0.x86-x64.fb2k-component"
      label="Version 0.2.0"
    />
    <DownloadListItem
      filename="foo_svg_services-0.1.0.x86-x64.fb2k-component"
      label="Version 0.1.0"
    />
  </DownloadList>
</DownloadLayout>
